        <!-- Edit Form -->
        <div class="popup2" id="popup3">

            <form method="post" action="/customers/edit/<%=data[0].customerNumber%>"  id="contact" >
                <h3>Edit data for customer</h3>

                <fieldset>
                    <input placeholder="Id" type="text" tabindex="1" value="<%=data[0].customerNumber%>" name="customerNumber" autofocus>
                </fieldset>

                <fieldset>
                    <input placeholder="Name Firm" type="text" value="<%=data[0].customerName%>" name="customerName">
                </fieldset>
                <fieldset>
                    <input placeholder="Last Name" type="text" value="<%=data[0].contactLastName%>" name="contactLastName">
                </fieldset>
                <fieldset>
                    <input placeholder="First Name" type="text" value="<%=data[0].contactFirstName%>" name="contactFirstName">
                </fieldset>
                <fieldset>
                    <input placeholder="Phone" type="text" value="<%=data[0].phone%>" name="phone">
                </fieldset>
                <fieldset>
                    <input placeholder="address1" type="text" value="<%=data[0].addressLine1%>"name="addressLine1"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="address2" type="text" value="<%=data[0].addressLine2%>" name="addressLine2"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="city" type="text" value="<%=data[0].city%>" name="city"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="state" type="text" value="<%=data[0].state%>" name="state"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="postalCode" type="text" value="<%=data[0].postalCode%>" name="postalCode"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="country" type="text" value="<%=data[0].country%>" name="country"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="sales Number" type="text" value="<%=data[0].salesRepEmployeeNumber%>" name="salesRepEmployeeNumber"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="creditLimit" type="text" value="<%=data[0].creditLimit%>" name="creditLimit"></input>
                </fieldset>

                <fieldset>
                    <button name="submit" type="submit" id="contact-submit">
                        Edit
                    </button>
                    <button type="submit" id="contact-submit" onClick="cancelAdd()">
                        Cancel
                    </button>
                </fieldset>
            </form>
