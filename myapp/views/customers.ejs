 <!-- Add Button -->
        <div class="bred">
            <button class="show_popup btn btn-danger"  rel="popup1">
                 +
            </button>
        </div>

        <div class="overlay_popup"></div>
  <!-- Add Form -->
        <div class="popup" id="popup1">

            <form method="post" id="contact" action="/customers" >
                <h3>Add data for customer</h3>
                <fieldset>
                    <input placeholder="Id" type="text" tabindex="1" name="customerNumber"  required autofocus>
                </fieldset>
                <fieldset>
                    <input placeholder="Name Firm" type="text" name="customerName" tabindex="2" required>
                </fieldset>
                <fieldset>
                    <input placeholder="Last Name" type="text" name="contactLastName" tabindex="3" required>
                </fieldset>
                <fieldset>
                    <input placeholder="First Name" type="text" name="contactFirstName" tabindex="4" required>
                </fieldset>
                <fieldset>
                    <input placeholder="Phone" type="text" name="phone" tabindex="5" required>
                </fieldset>
                <fieldset>
                    <input placeholder="address1" type="text" name="addressLine1" tabindex="6" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="address2" type="text" name="addressLine2" tabindex="7" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="city" type="text" name="city" tabindex="8" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="state" type="text" name="state" tabindex="9" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="postalCode" type="text" name="postalCode" tabindex="10" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="country" type="text" name="country" tabindex="11" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="sales Number" type="text" name="salesRepEmployeeNumber" tabindex="12" required></input>
                </fieldset>
                <fieldset>
                    <input placeholder="creditLimit" type="text" name="creditLimit" tabindex="13" required></input>
                </fieldset>

                <fieldset>
                    <button name="submit" type="submit" id="contact-submit">
                        add
                    </button>
                    <button type="submit" id="contact-submit" onClick="cancelAdd()">
                        cancel
                    </button>

                </fieldset>
            </form>
        </div>

 <!-- Edit Form -->
        <div class="popup2" id="popup3">

            <form method="post" id="contact" action="/customers/edit/<%=data[0].customerNumber%>" >
                <h3>Edit data for customer</h3>
                <fieldset>
                    <input placeholder="Id" type="text" tabindex="1" value="<%=data[0].customerNumber%>" name="customerNumber" autofocus>
                </fieldset>
                <fieldset>
                    <input placeholder="Name Firm" type="text" value="<%=data[0].customerNumber%>" name="customerName">
                </fieldset>
                <fieldset>
                    <input placeholder="Last Name" type="text" value="<%=data[0].contactLastName%>" name="contactLastName">
                </fieldset>
                <fieldset>
                    <input placeholder="First Name" type="text" value="<%=data[0].contactFirstName%>" name="contactFirstName">
                </fieldset>
                <fieldset>
                    <input placeholder="Phone" type="text" value="<%=data[0].phone%>" name="phone">
                </fieldset>
                <fieldset>
                    <input placeholder="address1" type="text" value="<%=data[0].addressLine1%>" name="phone" name="addressLine1"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="address2" type="text" value="<%=data[0].addressLine2%>" name="addressLine2"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="city" type="text" value="<%=data[0].city%>" name="city"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="state" type="text" value="<%=data[0].state%>" name="state"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="postalCode" type="text" value="<%=data[0].postalCode%>" name="postalCode"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="country" type="text" value="<%=data[0].country%>" name="country"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="sales Number" type="text" value="<%=data[0].salesRepEmployeeNumber%>" name="salesRepEmployeeNumber"></input>
                </fieldset>
                <fieldset>
                    <input placeholder="creditLimit" type="text" value="<%=data[0].creditLimit%>" name="creditLimit"></input>
                </fieldset>

                <fieldset>
                    <button name="submit" type="submit" id="contact-submit">
                        Edit
                    </button>
                    <button type="submit" id="contact-submit" onClick="cancelAdd()">
                        Cancel
                    </button>
                </fieldset>
            </form>
        </div>

  <!-- View in loop data -->
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title"></h4>
                                <p class="category"></p>
                            </div>
                            <div class ="scrol">
                            <div class="content table-responsive table-full-width">
                                <table class="table table-hover table-bordered">
                                    <thead>
                                        <th>Id</th>
                                        <th>Name Firm</th>
                                        <th>Last Name</th>
                                        <th>First Name</th>
                                        <th>phone</th>
                                        <th>address1</th>
                                        <th>address2</th>
                                        <th>city</th>
                                        <th>state</th>
                                        <th>postalCode</th>
                                        <th>country</th>
                                        <th>sales Number</th>
                                        <th>creditLimit</th>
                                        <th>operation</th>
                                    </thead>

                                    <tbody>
                                    <% if(data.length){
                                    for(var i = 0; i < data.length; i++) { %>
                                    <tr>
                                        <th><%= data[i].customerNumber%></th>
                                        <th><%= data[i].customerName%></th>
                                        <th><%= data[i].contactLastName%></th>
                                        <th><%= data[i].contactFirstName%></th>
                                        <th><%= data[i].phone%></th>
                                        <th><%= data[i].addressLine1%></th>
                                        <th><%= data[i].addressLine2%></th>
                                        <th><%= data[i].city%></th>
                                        <th><%= data[i].state%></th>
                                        <th><%= data[i].postalCode%></th>
                                        <th><%= data[i].country%></th>
                                        <th><%= data[i].salesRepEmployeeNumber%></th>
                                        <th><%= data[i].creditLimit%></th>
                                        <th>
                                            <button type="button" href="../customers/delete/<%=data[i].customerNumber%>">
                                                d
                                            </button>
                                            <button type="button" class="show_popup2"  rel="popup3"  href="../customers/edit/<%=data[i].customerNumber%>">
                                                e
                                            </button>
                                        </th>
                                    </tr>
                                    <% }

                                    }else{ %>
                                    <tr>
                                        <td colspan="3">No user</td>
                                    </tr>
                                    <% } %>
                                    </tbody>
                                </table>

                               </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
